//-------------------------------------------------------------------------------
a=[1 1 -1; 0 1 3; -1 0 -2]
a=[1 1 -1; 2 -1 1; -1 2 2]
a=[1 1 ; 2 -1 ]
//-------------------------------------------------------------------------------
//Gauss Method
//-------------------------------------------------------------------------------
//-------------------------------------------------------------------------------
function [a] = gaus(a)
//primera matriz

  [n,m]=size(a)
  for i=2:n

    factor1=-a(i,1)/a(1,1)
    for j=1:m
      a(i,j)=a(i,j)+factor1*a(1,j)
    end
  end
endfunction
//-------------------------------------------------------------------------------
function [a] = gaus(a)
  [n,m]=size(a)
  for k=0:n-1
    for i=2+k:n
      if a(2+k,1+k)~=0 then
	factor1=-a(i,k+1)/a(k+1,k+1)
	for j=1+k:m
	  a(i, j)=a(i,j)+factor1*a(k+1,j)
	end
      end
    end
  end
endfunction
//-------------------------------------------------------------------------------
[b]=gaus(a)
//-------------------------------------------------------------------------------
//-------------------------------------------------------------------------------
// Crout Method
//-------------------------------------------------------------------------------
//-------------------------------------------------------------------------------
//primera matriz ejemplo
a=[1 1 1;4 3 -1; 3 5 3]
a=[1 1 0;2 1 3; 3 1 1]

u=zeros(3,3)
l=zeros(3,3)
[n,m]=size(a)
for i=1:n
  for j=1:m
      if j<i then
	l(j,i)=0
      else
	l(j,i)=a(j,i)
	for k=1,j-1
	  l(j,i)=l(j,i)-l(j,k)*u(k,i)
	end 
      end
  end
  for j=1,m
    if(j<i) then
      u(i,j)=0
    elseif(j==i)
      u(i,j)= 1
    else
      u(i,j)=a(i,j)/l(i,i)
      for k=0,i-1
	u(i,j)=u(i,j)-(l(i,k)*u(k,j)/l(i,i))
      end
    end
  end
end
//-------------------------------------------------------------------------------
